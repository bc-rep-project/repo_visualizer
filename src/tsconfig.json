{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom", 
      "dom.iterable", 
      "esnext"
    ],
    "allowJs": true, 
    "skipLibCheck": true,
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true, // Add this line
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx" 
  },
  "include": [
    "src"
  ]
}